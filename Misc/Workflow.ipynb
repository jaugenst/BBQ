   "source": [
    "base_image = Path(f\"/home/saka/Documents/Lab_stuff/confocal/exp2/stack.tif\").as_posix()\n",
    "open_image = f'open(\"{base_image}\")'\n",
    "imp = ij.py.run_macro(open_image)\n",
